 // Calcular d√≠gitos verificadores
    $soma1 = 0;
    $soma2 = 0;
    for ($i = 0; $i < 9; $i++) {
        $digito = intval($cpf[$i]);
        $soma1 += $digito * (10 - $i);
        $soma2 += $digito * (11 - $i);
    }
    $soma2 += intval($cpf[9]) * 2;
    
    $digito1 = ($soma1 * 10) % 11;
    $digito2 = ($soma2 * 10) % 11;
    if ($digito2 == 10) {
        $digito2 = 0;
    }
